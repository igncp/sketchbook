<!DOCTYPE html>
<html xmlns:xlink="http://www.w3.org/1999/xlink">
<head>
    <title>Diagrams: <%= projectName %> - <%= currentPath %></title>
    
    <%- include partials/_common_dependencies.ejs %>
    
    <script type="text/javascript" src="/vendors/react/react.min.js"></script>
    <script type="text/javascript" src="/vendors/react/react-dom.min.js"></script>
    <script type="text/javascript" src="/vendors/highlightjs/highlight.pack.js"></script>
    <script type="text/javascript" src="/vendors/diagrams-collections/dist/<%= diagramsFile %>.js"></script>
    
    <link rel="stylesheet" type="text/css" href="/vendors/highlightjs/styles/solarized_light.css">
    <link rel="stylesheet" type="text/css" href="/vendors/diagrams-collections/dist/diagrams.css">
    
    <script>
        var sketchbook = {};

        (function() {
            var sharedFilesPathsStr = '<%= sharedFilesPaths %>';
            sketchbook.sharedFilesPaths = sharedFilesPathsStr ? sharedFilesPathsStr.split(',') : [];
            sketchbook.diagramPath = "/projects/<%= projectName %><%= currentPath %>.js";
        })()
    </script>

    <script type="text/javascript" src="/dist/diagram.js"></script>
</head>
<body>
    <%- include partials/_header.ejs %>
    <div class="row">
    <%- include partials/_breadcrumb.ejs %>
    <h3 class="col-lg-12">
        <a id="diagram-configuration-trigger" href="#"></a> 
        <a href="#" id="diagram-fullscreen-trigger" class="svg-image" title="Enter fullscreen"><img src="/images/icons/fullscreen-free.svg" alt=""></a>
    </h3>
    </div>
    <div class="row" id="diagram-configuration-panel">
    </div>
    <div id="diagram-wrapper"></div>
    <div class="modal fade" id="sketchbook-modal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
      <div class="modal-dialog" role="document">
        <div class="modal-content">
          <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
          </div>
          <div class="modal-body">
          </div>
        </div>
      </div>
    </div>
</body>
</html>