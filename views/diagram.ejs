<!DOCTYPE html>
<html xmlns:xlink="http://www.w3.org/1999/xlink">
<head>
    <title>Diagrams: <%= projectName %> - <%= currentPath %></title>
    
    <%- include partials/_common_dependencies.ejs %>
    
    <script type="text/javascript" src="/vendors/lodash/lodash.js"></script>
    <script type="text/javascript" src="/vendors/rxjs/dist/rx.all.min.js"></script>
    <script type="text/javascript" src="/vendors/d3/d3.js"></script>
    <script type="text/javascript" src="/vendors/highlightjs/highlight.pack.js"></script>
    
    <script type="text/javascript" src="/vendors/react/react.js"></script>
    <script type="text/javascript" src="/vendors/react/react-dom.js"></script>
    
    <script type="text/javascript" src="/vendors/diagrams-collections/dist/<%= diagramsFile %>.js"></script>
    
    <link rel="stylesheet" type="text/css" href="/vendors/highlightjs/styles/solarized_light.css">
    <link rel="stylesheet" type="text/css" href="/vendors/diagrams-collections/dist/diagrams.css">
    
    <script>
        var sketchbook = {};

        <% if (sharedExists === true) { %>
            sketchbook.sharedPath = "/projects/<%= projectName %>/shared.js";
        <% } %>
        sketchbook.diagramPath = "/projects/<%= projectName %><%= currentPath %>.js";
    </script>
    
    <script type="text/javascript" src="/vendors/requirejs/require.js" data-main="/assets/js/diagram.js"></script>
</head>
<body>
    <%- include partials/_header.ejs %>
    <div class="row">
    <%- include partials/_breadcrumb.ejs %>
    <h3 class="col-lg-12">
        <a id="diagram-configuration-trigger" href="#"></a> 
        <a href="#" id="diagram-fullscreen-trigger" class="svg-image" title="Enter fullscreen"><img src="/assets/icons/fullscreen-free.svg" alt=""></a>
    </h3>
    </div>
    <div class="row" id="diagram-configuration-panel">
    </div>
    <div id="diagram-wrapper"></div>
</body>
</html>