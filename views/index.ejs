<!DOCTYPE html>
<html>
<head>
    <title>Projects</title>
    <%- include partials/_common_dependencies.ejs %>
</head>
<body>
<%- include partials/_header.ejs %>
<div class="col-lg-offset-1 col-lg-10">
<h2>Projects:</h2>
<ul>
    <% projects.forEach(function(project){ %>
        <li><a href="/<%= project.name %>"><%= project.name %></a></li>
    <% }); %>
</ul>
</div>
<style type="text/css">
    ul li {
        font-size: 30px;
    }
</style>
</body>
</html>